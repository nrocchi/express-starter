{
  "name": "express-starter",
  "version": "1.0.0",
  "description": "express-starter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "debug": "set DEBUG=express:* && npm run dev",
    "dev:migration": "set NODE_ENV=development&& npx sequelize-cli db:migrate",
    "dev:migration:undo": "set NODE_ENV=development&& npx sequelize-cli db:migrate:undo",
    "dev:migration:undo:all": "set NODE_ENV=development&& npx sequelize-cli db:migrate:undo:all",
    "dev:seed": "set NODE_ENV=development&& npx sequelize-cli db:seed:all",
    "dev:seed:undo": "set NODE_ENV=development&& npx sequelize-cli db:seed:undo",
    "dev:seed:undo:all": "set NODE_ENV=development&& npx sequelize-cli db:seed:undo:all",
    "dev:seed:debug": "set NODE_ENV=development&& npx sequelize-cli db:seed:all --debug",
    "dev": "set NODE_ENV=development&& node ./index.js",
    "dev:start": "set NODE_ENV=development&& nodemon ./index.js",
    "prod:migration": "set NODE_ENV=production && npx sequelize-cli db:migrate",
    "prod:migration:undo": "set NODE_ENV=production && npx sequelize-cli db:migrate:undo",
    "prod:migration:undo:all": "set NODE_ENV=production && npx sequelize-cli db:migrate:undo:all",
    "prod:seed": "set NODE_ENV=production&& npx sequelize-cli db:seed:all",
    "prod:seed:undo": "set NODE_ENV=production&& npx sequelize-cli db:seed:undo",
    "prod:seed:undo:all": "set NODE_ENV=production&& npx sequelize-cli db:seed:undo:all",
    "prod:seed:debug": "set NODE_ENV=production&& npx sequelize-cli db:seed:all --debug",
    "prod": "set NODE_ENV=production&& node ./index.js",
    "prod:start": "set NODE_ENV=production&& nodemon ./index.js",
    "test:db:create": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "test:migration": "set NODE_ENV=test&& npx sequelize-cli db:migrate",
    "test:migration:undo": "set NODE_ENV=test&& npx sequelize-cli db:migrate:undo",
    "test:migration:undo:all": "set NODE_ENV=test&& npx sequelize-cli db:migrate:undo:all",
    "test:seed": "set NODE_ENV=test&& npx sequelize-cli db:seed:all",
    "test:seed:undo": "set NODE_ENV=test&& npx sequelize-cli db:seed:undo",
    "test:seed:undo:all": "set NODE_ENV=test&& npx sequelize-cli db:seed:undo:all",
    "test:seed:debug": "set NODE_ENV=test&& npx sequelize-cli db:seed:all --debug",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --runInBand --detectOpenHandles",
    "pretest": "cross-env NODE_ENV=test npm run pretest:db:migrate:reset",
    "pretest:db:migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run test:migration && npm run test:seed",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "swagger": "set NODE_ENV=development&& node ./swagger.js"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "verbose": true
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.6.0",
    "@faker-js/faker": "^7.6.0",
    "apicache-plus": "^2.3.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-timeout-handler": "^2.2.2",
    "handlebars": "^4.7.7",
    "joi": "^17.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.8.0",
    "ora": "^5.4.0",
    "rootpath": "^0.1.2",
    "sequelize": "^6.3.4",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.5.4",
    "swagger-autogen": "^2.22.0",
    "swagger-ui-express": "^4.5.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.5.2",
    "supertest": "^6.3.3"
  }
}
